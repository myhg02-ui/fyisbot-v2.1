{
  "version": 2,
  "builds": [
    {
      "src": "api/check_emails.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/check_emails",
      "dest": "/api/check_emails.py"
    },
    {
      "src": "/panel",
      "dest": "/panel.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}