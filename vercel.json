{
  "version": 2,
  "builds": [
    {
      "src": "api/check_emails.py",
      "use": "@vercel/python"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/check_emails.py",
      "dest": "/api/check_emails.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
